\par DROP TABLE diak;
\par DROP TABLE osztaly;
\par 
\par CREATE TABLE osztaly
\par (
\par \tab evfolyam NUMBER(2),
\par \tab osztaly CHAR(1),
\par \tab osztalyfonok VARCHAR(32),
\par \tab terem NUMBER(3) UNIQUE,
\par \tab CONSTRAINT pk_osztaly PRIMARY KEY (evfolyam, osztaly),
\par \tab CONSTRAINT check_osztaly_evfolyam CHECK (evfolyam BETWEEN 9 and 12)
\par );
\par 
\par CREATE TABLE diak
\par (
\par \tab nev VARCHAR(32),
\par \tab anyja_neve VARCHAR(32),
\par \tab szuletes DATE,
\par \tab evfolyam NUMBER(2),
\par \tab osztaly CHAR(1),
\par \tab iranyitoszam NUMBER(4),
\par \tab lakcim VARCHAR(32),
\par \tab CONSTRAINT pk_diak PRIMARY KEY (nev, anyja_neve, szuletes),
\par \tab CONSTRAINT fk_diak_osztaly FOREIGN KEY (evfolyam, osztaly) REFERENCES osztaly (evfolyam, osztaly)
\par );
\par 
\par CREATE TABLE ertekeles
\par (
\par \tab nev VARCHAR(32),
\par \tab anyja_neve VARCHAR(32),
\par \tab szuletes DATE,
\par \tab datum DATE,
\par \tab targy VARCHAR(16),
\par \tab osztalyzat NUMBER(1),
\par \tab hianyzas NUMBER(1),
\par \tab CONSTRAINT pk_ertekeles PRIMARY KEY (nev, anyja_neve, szuletes, datum, targy),
\par \tab CONSTRAINT check_ertekeles_targy CHECK (targy IN ('Magyar', 'Matematika', 'Technika', 'Zene', 'Testnevel\'e9s')),
\par \tab CONSTRAINT check_ertekeles_osztalyzat CHECK (osztalyzat BETWEEN 1 AND 5),
\par \tab CONSTRAINT check_ertekeles_hianyzas CHECK (hianyzas IS NULL OR hianyzas IS NOT NULL AND osztalyzat IS NULL AND hianyzas <= 1),
\par \tab CONSTRAINT fk_ertekeles_diak FOREIGN KEY (nev, anyja_neve, szuletes) REFERENCES diak (nev, anyja_neve, szuletes)
\par );
