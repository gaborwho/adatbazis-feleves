\par CREATE TABLE tanuloparok AS SELECT * FROM diak;
\par ALTER TABLE tanuloparok ADD (par_nev VARCHAR(32), par_anyja_neve VARCHAR(32), par_szuletes DATE);
\par 
\par CREATE OR REPLACE VIEW diakatlag AS
\par SELECT
\par \tab t.nev, t.anyja_neve, t.szuletes,
\par \tab t.par_nev, t.par_anyja_neve, t.par_szuletes,
\par \tab t.evfolyam,
\par \tab a.atlag
\par FROM
\par \tab tanuloparok t,
\par (SELECT d.nev, d.anyja_neve, d.szuletes,
\par \tab AVG(e.osztalyzat) AS atlag
\par FROM tanuloparok d, ertekeles e
\par WHERE
\par \tab d.nev = e.nev AND
\par \tab d.anyja_neve = e.anyja_neve AND
\par \tab d.szuletes = e.szuletes
\par GROUP BY d.nev, d.anyja_neve, d.szuletes) a
\par WHERE
\par \tab t.nev = a.nev AND
\par \tab t.anyja_neve = a.anyja_neve AND
\par \tab t.szuletes = a.szuletes;
\par 
\par BEGIN
\par FOR i IN (
\par \tab SELECT * FROM diakatlag
\par \tab ORDER BY atlag DESC)
\par LOOP
\par SELECT par_nev INTO parnev FROM tanuloparok t
\par \tab WHERE t.nev = i.nev AND t.anyja_neve = i.anyja_neve AND t.szuletes = i.szuletes;
\par IF parnev IS NULL
\par THEN
\par \tab FOR j IN (
\par \tab \tab SELECT * FROM diakatlag
\par \tab \tab WHERE
\par \tab \tab \tab par_nev IS NULL AND
\par \tab \tab \tab evfolyam = i.evfolyam AND 
\par \tab \tab \tab nev != i.nev AND anyja_neve != i.anyja_neve AND szuletes != i.szuletes
\par \tab \tab ORDER BY atlag ASC)
\par \tab LOOP\tab \tab 
\par \tab \tab UPDATE tanuloparok SET
\par \tab \tab \tab par_nev = j.nev,
\par \tab \tab \tab par_anyja_neve = j.anyja_neve,
\par \tab \tab \tab par_szuletes = j.szuletes
\par \tab \tab WHERE
\par \tab \tab \tab nev = i.nev AND
\par \tab \tab \tab anyja_neve = i.anyja_neve
\par \tab \tab \tab AND szuletes = i.szuletes;
\par 
\par \tab \tab UPDATE tanuloparok SET
\par \tab \tab \tab par_nev = i.nev,
\par \tab \tab \tab par_anyja_neve = i.anyja_neve,
\par \tab \tab \tab par_szuletes = i.szuletes
\par \tab \tab WHERE
\par \tab \tab \tab nev = j.nev AND
\par \tab \tab \tab anyja_neve = j.anyja_neve
\par \tab \tab \tab AND szuletes = j.szuletes;
\par 
\par \tab \tab EXIT;
\par \tab END LOOP;
\par END IF;
\par END LOOP;
\par END;
\par /
